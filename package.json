{
  "type": "module",
  "name": "basiccanvas",
  "title": "BasicCanvas",
  "description": "Simple JavaScript canvas abstractions.",
  "version": "1.2.3",
  "main": "lib/BasicCanvas.js",
  "homepage": "https://github.com/Demonstrandum/BasicCanvas/",
  "author": {
    "name": "S. F. D. Knutsen",
    "url": "https://github.com/Demonstrandum/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Demonstrandum/BasicCanvas.git"
  },
  "keywords": [
    "BasicCanvas",
    "javascript",
    "browser",
    "library",
    "canvas",
    "art"
  ],
  "bugs": {
    "url": "https://github.com/Demonstrandum/BasicCanvas/issues"
  },
  "license": "GPL-2.0",
  "dependencies": {
    "canvas": "^2.6.1",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0"
  },
  "scripts": {
    "test": "xo"
  },
  "devDependencies": {
    "xo": "^0.21.1"
  },
  "xo": {
    "space": true,
    "rules": {
      "curly": "off",
      "camelcase": "off",
      "unicorn/filename-case": "off",
      "new-cap": "off",
      "no-mixed-operators": "off",
      "no-undef": "off",
      "import/extensions": "off",
      "max-params": "off",
      "no-warning-comments": "off",
      "no-multi-spaces": "off",
      "func-name-matching": "off",
      "func-names": "off",
      "no-use-extend-native/no-use-extend-native": "off",
      "no-extend-native": "off",
      "no-unused-expressions": "off",
      "unicorn/escape-case": "off"
    }
  }
}
