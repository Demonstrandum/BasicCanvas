{
  "scripts": {
    "test": "xo"
  },
  "devDependencies": {
    "xo": "^0.21.1"
  },
  "xo": {
    "space": true,
    "rules": {
      "camelcase": "off",
      "unicorn/filename-case": "off",
      "new-cap": "off",
      "no-mixed-operators": "off",
      "no-undef": "off",
      "import/extensions": "off",
      "max-params": "off",
      "no-warning-comments": "off",
      "no-multi-spaces": "off",
      "func-name-matching": "off",
      "func-names": "off",
      "no-use-extend-native/no-use-extend-native": "off",
      "no-extend-native": "off"
    }
  }
}
